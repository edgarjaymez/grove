{
	"name": "grove",
	"version": "v1.13.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"build-tokens": "node build-tokens.js"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./tokens.css": "./dist/tokens/tokens.css"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "5.0.0",
		"@eslint/compat": "2.0.2",
		"@eslint/js": "9.39.2",
		"@playwright/test": "1.58.1",
		"@storybook/addon-a11y": "10.2.4",
		"@storybook/addon-docs": "10.2.4",
		"@storybook/addon-svelte-csf": "5.0.10",
		"@storybook/addon-vitest": "10.2.4",
		"@storybook/sveltekit": "10.2.4",
		"@sveltejs/adapter-auto": "7.0.0",
		"@sveltejs/kit": "2.50.2",
		"@sveltejs/package": "2.5.7",
		"@sveltejs/vite-plugin-svelte": "6.2.4",
		"@terrazzo/cli": "2.0.0-beta.4",
		"@terrazzo/plugin-css": "2.0.0-beta.4",
		"@types/node": "25.2.0",
		"@vitest/browser-playwright": "4.0.18",
		"@vitest/coverage-v8": "4.0.18",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-storybook": "10.2.4",
		"eslint-plugin-svelte": "3.14.0",
		"globals": "17.3.0",
		"playwright": "1.58.1",
		"prettier": "3.8.1",
		"prettier-plugin-svelte": "3.4.1",
		"publint": "0.3.17",
		"storybook": "10.2.4",
		"svelte": "5.49.1",
		"svelte-check": "4.3.6",
		"typescript": "5.9.3",
		"typescript-eslint": "8.54.0",
		"vite": "7.3.1",
		"vite-plugin-devtools-json": "1.0.0",
		"vitest": "4.0.18",
		"vitest-browser-svelte": "2.0.2"
	},
	"keywords": [
		"svelte"
	]
}
